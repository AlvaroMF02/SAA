import matplotlib.pyplot as plt
import numpy as np
m = 2
b = 40

err = np.random.rand(100)-0.5 # para que no se pasen
x = np.array(range(0,100))
y = m*x + b
y1 = y + err * 20

plt.plot(x,y1,linestyle="none",marker=".")
plt.plot(x,y)
plt.plot(x,err*20+40,linestyle="none",marker=".")
plt.show()

plt.hist(err)
plt.show()




